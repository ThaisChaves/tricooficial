  "html_body": "<script>\nsetTimeout(function () {\n  if ($('#bodyCart').length) {\n    $('.shopping-conclusion').after('<div id=\"restUnits\" style=\"padding: 1rem; margin-bottom: 2rem; border: 1px solid #ccc\">Faltam <strong id=\"lessUnit\">5</strong> itens no carrinho para finalizar a compra</div>')\n    infoCountQuantity(countQuantity(cart))\n  }\n}, 500)\n\n$(\".product-quantity .form-control\").change(function () {\n  if ($('#bodyCart').length) {\n    setTimeout(function () {\n      infoCountQuantity(countQuantity(cart))\n    }, 500)\n  }\n})\n\nvar countQuantity = function (cart) {\n  var index = 0\n  var quantity = 0\n  if (typeof cart === 'object') {\n    for (var id in cart) {\n      index++\n      if (cart[id].length > 1) {\n        for (var i = 0; i < cart[id].length; i++) {\n          quantity += cart[id][i][1]\n        }\n      } else {\n        if (cart[id].length === 2) {\n          quantity += cart[id][0][1]\n        } else {\n          quantity += cart[id][0][1]\n        }\n      }\n      if (index === Object.keys(cart).length) {\n        break\n      }\n    }\n    return quantity\n  }\n}\n\nvar infoCountQuantity = function () {\n  var quantity = countQuantity(cart)\n  var totalUnit = 5\n  if (quantity < totalUnit) {\n    $('#goto_checkout').hide()\n    $('#restUnits').show()\n    var totalQuantity = totalUnit - quantity\n    $('#lessUnit').text(totalQuantity)\n  } else {\n    $('#goto_checkout').show()\n    $('#restUnits').hide()\n  }\n}\n</script>\n\n<div class=\"fwr\">\n<a href=\"javascript;:\" target=\"_blank\"><img src=\"https://cdn.ecvol.com/s/www.tricooficial.com.br/uploads/Igor-Favero.png\" title=\"Igor Agência Digital\"></a>\n</div>\n\n<script>\n$('.fwr').insertBefore('.credits a');\n</script>\n"
